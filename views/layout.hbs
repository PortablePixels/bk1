<!doctype html>
<html>
  <head>
    <title>Botkit</title>
    <link rel="stylesheet" href="/css/styles.css" />
    <script src="/components/angular/angular.min.js"></script>
    <script src="/components/angular-drag-and-drop-lists/angular-drag-and-drop-lists.min.js"></script>
    <script src="/js/botkit_admin.js"></script>

    <meta name="viewport" content="width=device-width, initial-scale=1.0">

  </head>
  <body ng-app="botkit_admin" ng-controller="app">
    <div class="ng-class: {active: ui.dialog_open}; dialog_overlay"> <!--  ng-click="closeDialog($event)" -->
      <div ng-include="ui.dialog_template"></div>
    </div>
    <div class="outside_wrapper">
      {{> menu}}
      <div class="content_wrapper">
          {{{body}}}
      </div>
    </body>
  </html>
